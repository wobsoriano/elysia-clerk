{
  "name": "elysia-clerk",
  "version": "0.8.6",
  "description": "Unofficial Clerk plugin for Elysia.",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/elysia-clerk#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/elysia-clerk.git"
  },
  "bugs": "https://github.com/wobsoriano/elysia-clerk/issues",
  "keywords": [
    "clerk",
    "typescript",
    "bun",
    "auth",
    "authentication",
    "passwordless",
    "session",
    "jwt"
  ],
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "bun run --preload ./setup.ts --watch ./dev/index.tsx",
    "build": "bun run build.ts",
    "prepublishOnly": "bun run build",
    "release": "changeset publish",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "format": "biome format src --write",
    "test": "bun test --preload ./setup.ts",
    "test:watch": "bun test --watch"
  },
  "peerDependencies": {
    "elysia": "^1.0.0"
  },
  "dependencies": {
    "@clerk/backend": "^1.17.2",
    "@clerk/shared": "^2.16.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/cli": "^2.27.10",
    "@elysiajs/eden": "^1.1.3",
    "@elysiajs/html": "^1.1.1",
    "@types/bun": "^1.1.13",
    "bun-plugin-dts": "^0.3.0",
    "elysia": "^1.1.25",
    "typescript": "5.5.4"
  }
}
